    "iconv-lite": "^0.6.3",
    "marked": "^12.0.2",
    "mime-types": "^2.1.35",
    "nanoid": "^5.0.7",
    "pinia": "^2.1.7",
    "socks-proxy-agent": "^8.0.3",
    "uglify-js": "^3.17.4",
