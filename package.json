    "vue-router": "^4.3.2",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/dom-view-transitions": "^1.0.4",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^4.17.21",
    "@types/mime-types": "^2.1.4",
